{
  "name": "@giscore/react",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./tanstack-query": {
      "types": "./src/tanstack-query/index.ts",
      "default": "./src/tanstack-query/index.ts"
    }
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./tanstack-query": {
        "types": "./dist/tanstack-query/index.d.ts",
        "import": "./dist/tanstack-query/index.js"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@giscore/core": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "@tanstack/react-query": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "devDependencies": {
    "@giscore/eslint-config": "workspace:*",
    "@giscore/tsconfig": "workspace:*",
    "@tanstack/react-query": "^5.80.6",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "tsup": "^8.3.0"
  }
}
